<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ContactViewActivity">

    <TextView
            android:text="Nombre"
            android:layout_width="195dp"
            android:layout_height="wrap_content"
            android:id="@+id/nameText" android:textSize="25sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="108dp" android:layout_marginStart="108dp" android:layout_marginTop="282dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Telefono"
            android:layout_width="195dp"
            android:layout_height="wrap_content"
            android:id="@+id/phoneText" android:textSize="25sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="108dp" android:layout_marginStart="108dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/nameText" android:textColor="@android:color/holo_blue_dark"/>
    <TextView
            android:text="Correo"
            android:layout_width="195dp"
            android:layout_height="wrap_content"
            android:id="@+id/mailText" android:textSize="25sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="108dp" android:layout_marginStart="108dp" android:layout_marginTop="42dp"
            app:layout_constraintTop_toBottomOf="@+id/phoneText" android:textColor="@android:color/holo_blue_dark"/>
    <Button
            android:text="Regresar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonBack" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="161dp"
            android:layout_marginStart="161dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="133dp" android:layout_marginTop="92dp"
            app:layout_constraintTop_toBottomOf="@+id/mailText"/>
</android.support.constraint.ConstraintLayout>